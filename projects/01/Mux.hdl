/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

// Or(And(a, Not(sel)), And(b, sel))
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=sel, out=nsel);
    And(a=a, b=nsel, out=t1);
    And(a=b, b=sel, out=t2);
    Or(a=t1, b=t2, out=out);
}